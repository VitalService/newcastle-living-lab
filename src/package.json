{
	"name": "living-lab",
	"version": "1.26.0-beta.3",
	"private": true,
	"description": "Living Lab project",
	"main": "server.js",
	"bin": "server.js",
	"pkg": {
		"scripts": [
			"routes/*.js",
			"includes/*.js"
		],
		"assets": [
			"views/**/*",
			"public/**/*"
		]
	},
	"scripts": {
		"pkg-mac": "pkg . --targets macos --out-path ../pkg/macos/",
		"pkg-win": "pkg . --targets win --out-path ../pkg/win/",
		"pkg-linux": "pkg . --targets linux --out-path ../pkg/linux/",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development server.js",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production server.js"
	},
	"dependencies": {
		"archiver": "^3.1.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"connect-ensure-login": "^0.1.1",
		"cookie-parser": "^1.4.4",
		"cross-env": "^6.0.3",
		"dateformat": "^3.0.3",
		"directory-tree": "^2.2.4",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"formidable": "^1.2.2",
		"jimp": "^0.9.5",
		"jquery": "^2.2.4",
		"lodash": "^4.17.15",
		"nunjucks": "^3.2.0",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"rimraf": "^2.7.1",
		"showdown": "^1.9.1",
		"slugify": "^1.4.0",
		"socket.io": "^2.3.0",
		"sqlite3": "^3.1.13",
		"unzipper": "^0.9.15"
	},
	"author": "Newcastle University",
	"license": "MIT",
	"devDependencies": {
		"gulp": "^4.0.2",
		"gulp-clean-css": "^3.9.2",
		"gulp-concat": "^2.6.1",
		"gulp-less": "^3.4.0",
		"gulp-plumber": "^1.2.0",
		"gulp-rename": "^1.2.2"
	}
}
